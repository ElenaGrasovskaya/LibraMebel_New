{
  "name": "libramebel",
  "version": "2.0.0",
  "description": "Furniture studio website",
  "main": "index.html",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:sass": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:sass": "postcss --use autoprefixer -b 'last 5 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:sass prefix:sass compress:css"
  },
  "author": "Elena Grasovskaya",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "concat": "^1.0.3"
  }
}
